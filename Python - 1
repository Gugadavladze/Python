class Calculator:
    def __init__(self, x, y):
        self.x = x
        self.y = y

    def add(self):
        return self.x + self.y
    def subtract(self):
        return self.x - self.y
    def multiply(self):
        return self.x * self.y
    def divide(self):
        return self.x / self.y

c=Calculator(2, 5)

print("ADD =", c.add())
print("Subtract =", c.subtract())
print("Multiply =", c.multiply())
print("Divide =", c.divide())
