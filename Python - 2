class Rectangle:
    def __init__(self, width, length):
        self.width = width
        self.length = length

    def area(self):
        return self.width * self.length

    def perimeter(self):
        return (self.width + self.length) * 2

    def info(self):
        return f"Width - {self.width}, Length - {self.length}, Area - {self.area()}, Perimeter - {self.perimeter()}"


r = Rectangle(7,10)

print(r.info())
